{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "react/forbid-prop-types": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off"
  }
}